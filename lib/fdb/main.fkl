(import (bin debug))

(define arg-list (cdr (argv)))

(define prompt "(fdb)"))

(if (null arg-list)
  (exit))

(define file-name (car arg-list))

(define rest-arg (cdr arg-list))

(define debug-ctx (make-dbg-ctx file-name rest-arg))

(do [(cmd (dbg-read debug-ctx prompt)
          (dbg-read debug-ctx prompt))]
  [(dbg-ctx-exit? debug-ctx)]
  )
