(define fib
  (local
    (define fib-cache #vd(0 1 1))
    (define (fib-cached n)
      (define len (length fib-cache))
      (if (<= len n) (dvec-insert! fib-cache len (- n -1 len) nil))
      (define r (dvec-ref fib-cache n))
      (if r r
        (dvec-set! fib-cache
                   n
                   (+ (fib-cached (-1+ n))
                      (fib-cached (- n 2))))))))

