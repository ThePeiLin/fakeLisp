(import (.. make-test))

(define msg-vec
  (make-test
    (_handle)
    (_dns)
    (_process)
    (_pipe)
    (_tcp)
    (_udp)
    (_fs)
    (_misc)))

(define msgs (vector->list msg-vec))

(when (memp car msgs)
  (define errmsg "")
  (foreach (lambda (m)
             (setq errmsg
               (append! errmsg
                        (format "\n\t%s" (cdr m)))))
           msgs)
  (throw 'fuv-error
         errmsg))
