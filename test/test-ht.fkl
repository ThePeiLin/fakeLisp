(import (.. lib ht))

(define hash-table (make-ht equal-hashv equal))

(prin1 (ht-hashv hash-table))
(newline)
(prin1 (ht-equal hash-table))
(newline)

(ht-set! hash-table 'foo 'bar)
(ht-set! hash-table 'foo 'foobar)
(ht-set! hash-table 'bar 'barfoo)

(princ "\nht-ref 'foo")
(newline)

(println (ht-ref hash-table 'foo))

(princ "\nht-ref 'foobar 'foofoo")
(newline)
(println (ht-ref hash-table 'foobar 'foofoo))

(princ "\nht-ref! 'foobar 'foofoo")
(newline)
(println (ht-ref! hash-table 'foobar 'foofoo))

(princ "\nht-ref! 'foobar 'barbar")
(newline)
(println (ht-ref! hash-table 'foobar 'barbar))

(princ "\nht-ref& 'foobar")
(newline)
(println (ht-ref& hash-table 'foobar))

(princ "\nht-ref& 'foofoo")
(newline)
(println (ht-ref& hash-table 'foofoo))

(newline)
(princ "before clear")
(newline)
(prin1 (ht->list hash-table))
(newline)
(prin1 (ht-keys hash-table))
(newline)
(prin1 (ht-values hash-table))
(newline)

(princ "after clear")
(newline)
(ht-clear! hash-table)
(prin1 (ht->list hash-table))
(newline)
(prin1 (ht-keys hash-table))
(newline)
(prin1 (ht-values hash-table))
(newline)
